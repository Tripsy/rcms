{
    "name": "tripsy/stub-chain",
    "description": "Used for creating entities based on custom stubs",
    "type": "library",
    "keywords": ["stub", "laravel", "class generation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel David"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Tripsy\\StubChain\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tripsy\\StubChain\\StubChainServiceProvider"
            ]
        }
    }
}
